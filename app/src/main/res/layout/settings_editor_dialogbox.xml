<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/facbric_settings"
            android:textAlignment="center"
            android:textSize="20dp"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/matrial_name"
                android:textAlignment="center"
                android:textSize="20dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/t2"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"

                    android:textAlignment="center"
                    android:textSize="20dp"
                    />
                <TextView
                    android:id="@+id/t3"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"


                    android:textSize="20dp"
                    />
                <TextView
                    android:id="@+id/t4"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"

                    android:textAlignment="center"
                    android:textSize="20dp"
                    />
                <TextView
                    android:id="@+id/t5"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:textSize="20dp"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/code"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/code"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center"
                android:hint="code" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/material_type"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/material"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:id="@+id/matlistTv"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_listMAt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/count"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/suta"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:id="@+id/sutalistTv"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_listSuta"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/dying"
                android:gravity="center"
                android:textSize="20dp" />

            <RadioGroup
                android:id="@+id/radiogroup"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:orientation="horizontal"
                android:gravity="center">
                <RadioButton
                    android:id="@+id/radio_id1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/YES"/>
                <RadioButton
                    android:id="@+id/radio_id2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/NO"/>
            </RadioGroup>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/lamination"
                android:gravity="center"
                android:textSize="20dp" />

            <RadioGroup
                android:id="@+id/radiogroup2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:orientation="horizontal"
                android:gravity="center">
                <RadioButton
                    android:id="@+id/radiobtn2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/YES"/>
                <RadioButton
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/NO"/>
            </RadioGroup>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/bohor"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/bohor"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_bohor"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/proti_goj"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/gojmullo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_protigoj"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".75"
                android:text="@string/wastage"
                android:gravity="center"
                android:textSize="20dp" />
            <SeekBar
                android:id="@+id/seekbartv1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="20sp" />
            <TextView
                android:id="@+id/waste"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="100%"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".75"
                android:text="@string/flactuation"
                android:gravity="center"
                android:textSize="20dp" />
            <SeekBar
                android:id="@+id/seekbartv2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:textSize="20sp" />
            <TextView
                android:id="@+id/flactiation"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="100%"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/brand"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/brand"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_brand"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/color"
                android:gravity="center"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/color"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.25"
                android:gravity="center"
                android:hint="code" />
            <TextView
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:text="+"
                android:gravity="center"
                android:textSize="30dp"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="271dp"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:orientation="horizontal">

            <Spinner
                android:id="@+id/spinner_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/save"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="@string/save"
                android:gravity="center"
                android:textSize="20dp" />



        </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
